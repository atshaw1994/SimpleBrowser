    <SolidColorBrush x:Key="Button.Foreground.Disabled" Color="#10ffffff"/>

    <SolidColorBrush x:Key="BaseButton.Background" Color="#3F3F46"/>
    <SolidColorBrush x:Key="BaseButton.Foreground" Color="#FFF1F1F1"/>
    <SolidColorBrush x:Key="BaseButton.Border" Color="#555555"/>
    <SolidColorBrush x:Key="BaseButton.Border.MouseOver" Color="#007ACC"/>
    <SolidColorBrush x:Key="BaseButton.Pressed" Color="#007ACC"/>


    <SolidColorBrush x:Key="TabItem.Static.Border" Color="#80ffffff"/>
    <SolidColorBrush x:Key="TabItem.Hover.Background" Color="#2d2e33"/>
    <SolidColorBrush x:Key="TabItem.MouseOver.Border" Color="#FF3E3E40"/>
    <SolidColorBrush x:Key="TabItem.Selected.Background" Color="#353539"/>
    <SolidColorBrush x:Key="TabItem.Selected.Border" Color="#60ffffff"/>

    <SolidColorBrush x:Key="TextBox.Background" Color="#FF333337"/>
    <SolidColorBrush x:Key="TextBox.Foreground" Color="#FFF1F1F1"/>
