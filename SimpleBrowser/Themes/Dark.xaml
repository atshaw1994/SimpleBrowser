<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Global.Background" Color="#24262b"/>
    <SolidColorBrush x:Key="Global.Foreground" Color="#60ffffff"/>
    <SolidColorBrush x:Key="MDL2Button.Overlay" Color="#60ffffff" />
    <SolidColorBrush x:Key="MDL2Button.Overlay.MouseOver" Color="#90FF0000" />
    <SolidColorBrush x:Key="MDL2Button.Overlay.Pressed" Color="#80ffffff" />
    <SolidColorBrush x:Key="MDL2Button.Overlay.Stop" Color="#90FF0000"/>
    <SolidColorBrush x:Key="WindowMainPanel.Border" Color="#7Dffffff"/>
    <SolidColorBrush x:Key="MouseOverOverlayBackgroundBrush" Color="#33ffffff" />
    <SolidColorBrush x:Key="PressedOverlayBackgroundBrush" Color="#44ffffff" />
    <SolidColorBrush x:Key="MouseOverWindowCloseButtonBackgroundBrush" Color="#e81123" />
    <SolidColorBrush x:Key="PressedWindowCloseButtonBackgroundBrush" Color="#f1707a" />
